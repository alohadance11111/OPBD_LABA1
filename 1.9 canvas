import tkinter as tk

window = tk.Tk()
window.title("Дом с солнцем и травой")

canvas = tk.Canvas(window, width=600, height=400)
canvas.pack()

canvas.create_rectangle(0, 300, 600, 400, fill="green")

canvas.create_oval(500, 50, 550, 100, fill="yellow")

canvas.create_rectangle(200, 200, 400, 400, fill="red")

canvas.create_polygon(200, 200, 300, 100, 400, 200, fill="brown")

canvas.create_rectangle(280, 300, 320, 400, fill="brown")

canvas.create_rectangle(220, 220, 280, 280, fill="white")
canvas.create_rectangle(320, 220, 380, 280, fill="white")

window.mainloop()
